{
	"name": "problem5-backend",
	"version": "1.0.0",
	"description": "A crude server with CRUD operations",
	"main": "dist/src/index.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/src/index.js",
		"dev": "ts-node -r dotenv/config src/index.ts",
		"dev:tsx": "tsx --env-file=.env src/index.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest __tests__ --coverage",
		"test:unit": "jest __tests__",
		"test:integration": "jest __tests__/integration",
		"test:e2e": "jest tests/e2e.test.ts --testTimeout=60000",
		"test:e2e:docker": "docker-compose -f docker-compose.test.yml build && docker-compose -f docker-compose.test.yml --profile test up --abort-on-container-exit --exit-code-from e2e-tests --remove-orphans && docker-compose -f docker-compose.test.yml down -v",
		"test:load:docker": "docker-compose -f docker-compose.test.yml build && docker-compose -f docker-compose.test.yml --profile load up --abort-on-container-exit --exit-code-from load-test --remove-orphans && docker-compose -f docker-compose.test.yml down -v",
		"test:load:internal": "node dist/scripts/load-test.js",
		"test:load:k6": "k6 run scripts/load-test-k6.js",
		"test:load:k6:docker": "docker-compose -f docker-compose.test.yml build && docker-compose -f docker-compose.test.yml --profile k6 up --abort-on-container-exit --exit-code-from k6-load-test --remove-orphans && docker-compose -f docker-compose.test.yml down -v",
		"sonar": "sonar-scanner",
		"docker:scan": "docker-scan .",
		"lint": "eslint src/**/*.ts tests/**/*.ts scripts/**/*.ts",
		"lint:fix": "eslint src/**/*.ts tests/**/*.ts scripts/**/*.ts --fix",
		"ci": "npm run lint && npm test && npm run build",
		"typeorm": "typeorm-ts-node-esm -d src/db/data-source-cli.js",
		"migration:generate": "npm run typeorm -- migration:generate",
		"migration:create": "npm run typeorm -- migration:create",
		"migration:run": "npm run typeorm -- migration:run",
		"migration:revert": "npm run typeorm -- migration:revert"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/compression": "^1.8.1",
		"@types/express-rate-limit": "^6.0.2",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/redis": "^4.0.11",
		"bcrypt": "^6.0.0",
		"compression": "^1.8.1",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-rate-limit": "^8.2.1",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.11.3",
		"prom-client": "^15.1.3",
		"redis": "^5.10.0",
		"reflect-metadata": "^0.2.2",
		"sqlite3": "^5.1.6",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"winston": "^3.11.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.8",
		"@types/node": "^20.10.0",
		"@types/pg": "^8.10.9",
		"@types/sqlite3": "^3.1.11",
		"@types/supertest": "^2.0.16",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"@typescript-eslint/eslint-plugin": "^8.48.0",
		"@typescript-eslint/parser": "^8.48.0",
		"docker-scan": "^1.1.0",
		"eslint": "^9.39.1",
		"jest": "^29.7.0",
		"k6": "^0.0.0",
		"sonarqube-scanner": "^4.3.2",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"tsx": "^4.21.0",
		"typeorm": "^0.3.27",
		"typescript": "^5.3.3"
	}
}
